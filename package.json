{
  "name": "firebaseui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-library && npx copy README.MD dist/firebaseui-angular-library",
    "prerelease": "cd projects/firebaseui-angular-library && npm version prerelease --preid=beta",
    "_prepatch": "cd projects/firebaseui-angular-library && npm version prepatch  --preid=beta",
    "patch": "cd projects/firebaseui-angular-library && npm version patch",
    "_preminor": "cd projects/firebaseui-angular-library && npm version preminor  --preid=beta",
    "minor": "cd projects/firebaseui-angular-library && npm version minor",
    "_premajor": "cd projects/firebaseui-angular-library && npm version premajor  --preid=beta",
    "major": "cd projects/firebaseui-angular-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish dist/firebaseui-angular-library",
    "publish-dist-beta": "npm run build-lib && npm publish dist/firebaseui-angular-library --tag beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "9.1.0",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "core-js": "^3.3.3",
    "firebase": "^7.13.2",
    "firebaseui": "^4.2.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copy": "^0.3.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.4.1",
    "tslint": "5.17.0",
    "typescript": "3.8.3"
  }
}
