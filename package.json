{
  "name": "firebaseui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-library --configuration production && npx copy README.MD dist/firebaseui-angular-library",
    "prerelease": "cd projects/firebaseui-angular-library && npm version prerelease --preid=beta",
    "_prepatch": "cd projects/firebaseui-angular-library && npm version prepatch  --preid=beta",
    "patch": "cd projects/firebaseui-angular-library && npm version patch",
    "_preminor": "cd projects/firebaseui-angular-library && npm version preminor  --preid=beta",
    "minor": "cd projects/firebaseui-angular-library && npm version minor",
    "_premajor": "cd projects/firebaseui-angular-library && npm version premajor  --preid=beta",
    "major": "cd projects/firebaseui-angular-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish ./dist/firebaseui-angular-library",
    "publish-dist-beta": "npm run build-lib && npm publish ./dist/firebaseui-angular-library --tag beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "firebase": "^10.7.2",
    "firebaseui": "^6.1.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "17.3.12",
    "@types/jasmine": "^3.6.1",
    "@types/node": "^14.14.6",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^17.1.1",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "6.1.3",
    "typescript": "5.4.5"
  }
}