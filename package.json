{
  "name": "firebaseui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-library --prod && npx copy README.MD dist/firebaseui-angular-library",
    "prerelease": "cd projects/firebaseui-angular-library && npm version prerelease",
    "prepatch": "cd projects/firebaseui-angular-library && npm version prepatch",
    "patch": "cd projects/firebaseui-angular-library && npm version patch",
    "preminor": "cd projects/firebaseui-angular-library && npm version preminor",
    "minor": "cd projects/firebaseui-angular-library && npm version minor",
    "premajor": "cd projects/firebaseui-angular-library && npm version premajor",
    "major": "cd projects/firebaseui-angular-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish dist/firebaseui-angular-library"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.7",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/fire": "5.0.0",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/router": "6.1.7",
    "core-js": "^2.5.7",
    "firebase": "^5.4.2",
    "firebaseui": "^3.4.0",
    "rxjs": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular-devkit/build-ng-packagr": "^0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.1.1",
    "codelyzer": "4.4.3",
    "copy": "^0.3.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "1.1.0",
    "ng-packagr": "^3.0.6",
    "protractor": "^5.4.0",
    "ts-node": "7.0.0",
    "tsickle": "0.32.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "~2.9.2"
  }
}
